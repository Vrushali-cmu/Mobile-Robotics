figure(1)
kp = robotKeypressDriver(gcf);
vGain = 1;
robot_name = 'nano';
robot = neato(robot_name);
pause(2);
tstart = tic;
robotKeypressDriver.drive(robot,vGain);
%{
while true
    robotKeypressDriver.drive(robot,vGain);
    if toc(tstart)>10
        disp('Time up');
        break;
    end
end
%}

while true